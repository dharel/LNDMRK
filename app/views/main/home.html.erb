<section class='hero-map'>
  <h1 class='title'>invest any amount in any property. worldwide.</h1>
  <div class='hero-map-container'>
    <div class='filters-container'>
      <div class='filter income'>income</div>
      <div class='filter growth'>growth</div>
      <div class='filter income-growth'>income & growth</div>
    </div>
    <div class='search-container'>
      <input type="text" class='search' placeholder='Search address, city, state.'>
    </div>
    <div class='search-info'>
      <h1 class='search-button'>search</h1>
      <!-- <h6 class='subtitle'>or <br> click on the map</h6>
      <div id='diamond'></div>
      <div id='line'> </div> -->
    </div>
  </div>
</section>

<section-carousel class='carousel-directive' assets='assets' pagination='pagination'></section-carousel>

<section class='how-it-works'>
  <div class='container'>
    <div class='line'></div>
    <div class='title'>how it works</div>
    <div class='row-wrap'>
      <div class='info'>
        <div class='image search'></div>
        <div class='info-title'>search properties</div>
        <div class='text'>
          <p>
            Evaluate and compare properties' <br>
            risk and return
          </p>
          <div id='diamond'></div>
          <p>
            Place unsolicited bids on any <br>
            property in the world
          </p>
        </div>
      </div>
      <div class='info'>
        <div class='image invest'></div>
        <div class='info-title'>invest</div>
        <div class='text'>
          <p>Set bid price</p>
          <div id='diamond'></div>
          <p>Find slimie properties</p>
          <div id='diamond'></div>
          <p>Balance and customize you portfolio</p>        
        </div>
      </div>
      <div class='info'>
        <div class='image recieve'></div>
        <div class='info-title'>receive income</div>
        <div class='text'>
          <p>Direct deposit into your bank <br> account</p>
          <div id='diamond'></div>
          <p>Get SMS notifications on deposits</p>
          <div id='diamond'></div>
          <p>Vote on investors' representatives</p>        
        </div>
      </div>
      <div class='info exit'>
        <div class='image exit'></div>
        <div class='info-title'>exit</div>
        <div class='text'>
          <p>Set ask price</p>
          <div id='diamond'></div>
          <p>Get bids</p>     
        </div>
      </div>
    </div>
  </div>
</section>

<section class='statistics'>
  <div class='container'>
    <div class='column'>
