        <div class='pie-image'></div>
        <h2>453M</h2>
        <h4 class='growth'>growth</h4>
        <h4 class='income'>income</h4>
        <h4 class='income-growth'>income & growth</h4>
        <%= image_tag('line.png',class: 'line growth', width: '90', alt: '') %>
        <%= image_tag('line.png',class: 'line income', width: '90', alt: '') %>
        <%= image_tag('line.png',class: 'line income-growth', width: '90', alt: '') %>
      </div>
    </div>
    <div class='column'>
      <h3>average portfolio</h3>
      <div class='icon portfolio'></div>
      <h1>$53,327</h1>
    </div>
  </div>
</section>

